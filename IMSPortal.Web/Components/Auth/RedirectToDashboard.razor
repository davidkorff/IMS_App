@inject NavigationManager NavigationManager
@implements IDisposable

@code {
    private bool _navigated = false;

    protected override void OnInitialized()
    {
        if (!_navigated)
        {
            _navigated = true;
            NavigationManager.NavigateTo("/dashboard", replace: true);
        }
    }

    public void Dispose()
    {
        // Cleanup if needed
    }
} 