Content-Type: text/xml; charset=utf-8
Content-Length: length

<?xml version="1.0" encoding="utf-8"?>
<LoginReturn xmlns="http://tempuri.org/IMSWebServices/Logon">
  <UserGuid>guid</UserGuid>
  <Token>guid</Token>
</LoginReturn>
