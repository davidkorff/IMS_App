  <soap12:Header>
    <TokenHeader xmlns="http://tempuri.org/IMSWebServices/QuoteFunctions">
      <Token>guid</Token>
      <Context>string</Context>
    </TokenHeader>
  </soap12:Header>
  <soap12:Body>
    <UpdateQuoteRiskInformation xmlns="http://tempuri.org/IMSWebServices/QuoteFunctions">
      <QuoteGuid>guid</QuoteGuid>
      <RiskInformation>
        <PolicyName>string</PolicyName>
        <CorporationName>string</CorporationName>
        <DBA>string</DBA>
        <Salutation>string</Salutation>
        <FirstName>string</FirstName>
